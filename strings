str1="Hello world"
print(str1)
str2='1 2 3 4 5 6' #Using alternate quotes
print(str2)
str4=str1+str2 #Adding strings
print(str4)
greetings=4*"Hi " #Multiplying strings
print(greetings)
str5="When in the course of human events"
print(len(str5)
print(str5[0])
print(str5[24:])
print(str5[5:17]) 
